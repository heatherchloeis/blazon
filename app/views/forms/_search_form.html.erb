<%= form_tag search_path, local: true, class: "form-inline my-2 search", method: :get do %>
	<%= text_field_tag :q, nil, placeholder: "Search Canary", data: { behavior: "autocomplete" }, html: { autocomplete: "on" }, class: "form-control form-control-sm search" %>
	<%= button_tag do %>
		<%= icon('fas', 'search') %>
	<% end %>
<% end %>